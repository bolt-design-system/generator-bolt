{
  "name": "<%= props.packageName %>",
  "version": "0.0.0",
  "description": "<%= props.description %>",
  "repository": {
    "type": "git",
    "url": "git+<%= props.url %>"
  },
  "author": "<%= props.author_name %> <<%= props.author_mail %>>",
  "license": "MIT",
  "bugs": {
    "url": "<%= props.gitUrl %>/issues"
  },
  "keywords": [
    "design system",
    "ui-toolkit",
    "bem",
    "itcss",
    "bemit",
    "scss",
    "css",<% if (props.twig) { %>
    "twig",<% } %><% if (props.twig) { %>
    "html",<% } %><% if (props.javascript) { %>
    "javascript",<% } %><% if (props.javascript) { %>
    "js",<% } %>
    "pegakit",
    "<%= props.packageName %>",
    "<%= props.names.kebabcase.default %>"
  ]
}
